{
  "env": {
    "node": true,
    "jquery": true
  },
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "prefer-arrow-callback": 1,
    "quotes": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-debugger": 1,
    "no-var": 1,
    "semi": [1, "always"],
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "no-param-reassign": 0,
    "no-shadow": 1,
	"arrow-body-style": ["error", "always"],
    "comma-dangle": 0,
    "prefer-template": 0,
    "import/newline-after-import": 0,
	"consistent-return": 0
  }
}
